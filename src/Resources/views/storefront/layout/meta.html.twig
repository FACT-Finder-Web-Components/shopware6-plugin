{% sw_extends '@Parent/storefront/layout/meta.html.twig' %}

{% block layout_head_javascript_tracking %}
  <script>
    document.addEventListener('ffReady', function (e) {
      e.factfinder.sdk = 'sw6-v5.0.0';
      e.factfinder.communication.fieldRoles = {{ page.extensions.factfinder.field_roles|json_encode|raw }};
    });
  </script>
  <script src="{{ asset('ff-web-components/vendor/custom-elements-es5-adapter.js', '@FactFinder') }}"></script>
  <script src="{{ asset('ff-web-components/vendor/webcomponents-loader.js', '@FactFinder') }}"></script>
  <script src="{{ asset('ff-web-components/bundle.js', '@FactFinder') }}" defer></script>
  <style>[unresolved]{display:none}</style>

  {{ parent() }}
{% endblock %}
